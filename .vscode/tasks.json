{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/Fe${fileDirname}\\compile\\executable\\${fileBasenameNoExtension}.exe",
                "${fileDirname}/*.c",
                "${fileDirname}/hardware/*.c",
                "${file}",
                "/I ${fileDirname}\\SDL3-3.2.0\\include\\SDL3",
                "/link ${fileDirname}\\SDL3-3.2.0\\lib\\x86\\SDL3.lib ${fileDirname}\\SDL3-3.2.0\\lib\\x86\\SDL2_ttf.lib"
            ],
            "options": {
                "cwd": "${fileDirname}\\compile\\options"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}